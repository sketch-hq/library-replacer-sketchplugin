{
  "name": "library-replacer",
  "version": "1.0.1",
  "description": "Quickly replace instances of shared Symbols and Styles.",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Library Replacer",
    "manifest": "src/manifest.json",
    "main": "library-replacer.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.6.0"
  },
  "author": "Sketch",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/BohemianCoding/library-replacer-sketchplugin"
  }
}
